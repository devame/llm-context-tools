{
  "name": "llm-context-analyzer",
  "version": "0.1.0",
  "description": "LLM-optimized code analysis system - hybrid SCIP + custom semantic analysis",
  "type": "module",
  "scripts": {
    "analyze": "scip-typescript index --infer-tsconfig --output .llm-context/index.scip && node .llm-context/transformer.js && node .llm-context/summarizer.js",
    "query": "node .llm-context/query.js",
    "parse-scip": "node .llm-context/scip-parser.js",
    "transform": "node .llm-context/transformer.js",
    "summarize": "node .llm-context/summarizer.js"
  },
  "keywords": [
    "llm",
    "code-analysis",
    "ast",
    "scip",
    "code-intelligence",
    "semantic-analysis"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@sourcegraph/scip-typescript": "^0.4.0",
    "protobufjs": "^7.5.4"
  }
}
